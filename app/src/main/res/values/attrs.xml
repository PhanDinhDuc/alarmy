<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="BaseTextView" />

    <declare-styleable name="TabbarView">
        <attr name="tabSelectedColor" format="color" />
        <attr name="defaultColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomBottomView">
        <attr name="title" format="string" />
        <attr name="icon_bottom" format="reference" />
        <attr name="icon_bottom_show" format="reference" />
        <attr name="icon_bottom_selected" format="reference" />
        <attr name="isBottomSelected" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WheelPicker" tools:ignore="DuplicateDefinition">
        <attr name="fadingEdgeEnabled" format="boolean" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="selectedTextColor" format="color" />
        <attr name="selectedTextScale" format="float" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="typeface" format="enum">
            <enum name="normal" value="0" />
            <enum name="sans" value="1" />
            <enum name="serif" value="2" />
            <enum name="monospace" value="3" />
        </attr>
        <attr name="wheelItemCount" format="integer" />
        <attr name="wrapSelectorWheel" format="boolean" />
        <attr name="minValidIndex" format="integer" />
        <attr name="maxValidIndex" format="integer" />
        <attr name="align">
            <enum name="LEFT" value="0" />
            <enum name="CENTER" value="1" />
            <enum name="RIGHT" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTimer">
        <attr name="text_size" format="integer" />
        <attr name="view_background" format="boolean" />
        <attr name="count_down" format="integer" />
    </declare-styleable>

    <declare-styleable name="HeaderView">
        <attr name="cs_title" format="reference" />
        <attr name="action_right_title" format="string" />
        <attr name="src_right" format="reference" />
        <attr name="src_left" format="reference" />
        <attr name="title_text_size" format="dimension" />
        <attr name="title_gravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
        </attr>
        <attr name="right_action_type" format="enum">
            <enum name="none" value="0" />
            <enum name="image" value="1" />
            <enum name="text" value="2" />
        </attr>
        <attr name="left_action_type" format="enum">
            <enum name="none" value="0" />
            <enum name="image" value="1" />
            <enum name="text" value="2" />
        </attr>


    </declare-styleable>
    <declare-styleable name="BaseEditText" />

    <declare-styleable name="BottomView">
        <attr name="bottom_selected" format="boolean" />
        <attr name="title_bottom" format="string" />
        <attr name="icon" format="reference" />
        <attr name="icon_select" format="reference" />
    </declare-styleable>

    <declare-styleable name="NumberEquationsView">
        <attr name="minValue" format="float|reference|integer" />
        <attr name="maxValue" format="float|reference|integer" />
        <attr name="currentValue" format="string" />
        <attr name="mTitle" format="string" />
        <attr name="mTitle2" format="string" />
    </declare-styleable>

    <declare-styleable name="NumPadView">
        <attr name="text_sizes" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="text_ripple_effect" format="boolean" />
        <attr name="text_style">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="font_path" format="string" />
        <attr name="drawable_left" format="boolean" />
        <attr name="drawable_right" format="boolean" />
        <attr name="left_icon" format="reference" />
        <attr name="right_icon" format="reference" />
        <attr name="left_icon_tint" format="color" />
        <attr name="right_icon_tint" format="color" />
        <attr name="left_icon_ripple_effect" format="boolean" />
        <attr name="right_icon_ripple_effect" format="boolean" />
        <attr name="background_resource" format="reference" />
    </declare-styleable>

    <declare-styleable name="TabBarItemView">
        <attr name="tab_icon" format="reference" />
        <attr name="tab_icon_selected" format="reference" />
        <attr name="tab_text" format="string" />
    </declare-styleable>
</resources>