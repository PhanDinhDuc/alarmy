<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ic_thumb_quick_alarm" android:state_checked="true">
        <shape android:dither="true" android:shape="oval" android:useLevel="false" android:visible="true">
            <stroke android:width="1dp" android:color="@color/newtral_3" />
            <size android:height="20dp"  android:width="20dp"/>
        </shape>

    </item>
    <item android:drawable="@drawable/ic_thumb_quick_alarm" android:state_checked="false">
        <shape android:dither="true" android:shape="oval" android:useLevel="false" android:visible="true">
            <stroke android:width="1dp" android:color="@color/newtral_3" />
        </shape>
    </item>
</selector>